{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "prefer-destructuring": 0,
    "class-methods-use-this": 0,
    "max-len": 0,
    "no-multiple-empty-lines": 0,
    "object-shorthand": 0,
    "padded-blocks": 0
  }
}

